<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    android:id="@+id/landing_screen_linear_layout_main"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/Background">

    <RelativeLayout
        android:id="@+id/signin_relative_layout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/signin_topbar_linear_layout"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/_70sdp"

            android:background="@color/TopeBar"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginLeft="@dimen/back_btn_left_margin">

                <RelativeLayout
                    android:id="@+id/sign_in_back_btn"
                    android:layout_width="90dp"
                    android:layout_height="@dimen/screens_top_bar_height_mdpi"

                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/back_btn_left_margin"
                    android:clickable="true"
                    android:onClick="pressBackButton">

                    <ImageView
                        android:id="@+id/bmenu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/back_btn_left_margin"
                        android:background="@drawable/main_back_btn"
                        android:clickable="true"
                        android:onClick="pressBackButton" />
                </RelativeLayout>

                <TextView
                    android:id="@+id/sign_in_top_bar_text_view"
                    style="@style/header_text_view"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_centerVertical="true"
                    android:text="@string/screen_three_top_bar"
                    android:textAppearance="?android:attr/textAppearanceMedium" />


                <RelativeLayout
                    android:id="@+id/sign_in_back_btn_home"
                    android:layout_width="90dp"
                    android:layout_height="@dimen/screens_top_bar_height_mdpi"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/back_btn_left_margin"
                    android:clickable="true"
                    android:onClick="gotohomeScreen">

                    <ImageView
                        android:id="@+id/bmenuHOme"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/back_btn_left_margin"
                        android:background="@drawable/home"
                        android:clickable="true"
                        android:onClick="gotohomeScreen" />
                </RelativeLayout>


            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/signin_feilds_relative_layout"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_below="@+id/signin_relative_layout"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/logo_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="4dp"
            android:background="@drawable/logo"
            android:visibility="gone" />

        <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="10dp"
            android:scrollbarAlwaysDrawVerticalTrack="true"
            android:scrollbarFadeDuration="60000"
            android:scrollbars="vertical">

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"

                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="1dp"

                    android:orientation="vertical"
                    android:scaleType="fitXY">

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"

                        android:orientation="vertical"
                        android:scaleType="fitXY">

                        <EditText
                            android:id="@+id/sign_up_first_name_textfield"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="10dp"
                            android:background="@drawable/edittextbg"
                            android:hint="@string/sign_up_first_name_textfield_label"
                            android:inputType="textPersonName"
                            android:maxLength="100"
                            android:paddingLeft="10dp"
                            android:scaleType="fitXY"
                            android:textColorHint="@color/LightGrey"
                            android:visibility="gone">

                            <requestFocus />
                        </EditText>

                        <com.google.android.material.floatingactionbutton.FloatingActionButton
                            android:id="@+id/btn_RedialCall"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:backgroundTint="@color/black_color"
                            android:contentDescription="Call"
                            android:onClick="DialUserNumber"
                            android:clickable="true"
                            android:src="@drawable/call_icon_foreground"
                            android:tint="@color/white_color" />
                        <com.google.android.material.floatingactionbutton.FloatingActionButton
                            android:id="@+id/btn_AddReportQuestionnaire"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toLeftOf="@id/btn_RedialCall"
                            android:contentDescription="report"
                            android:src="@drawable/img_register"
                            android:onClick="launchReport"
                            android:clickable="true"
                            android:tint="@color/white_color" />
                        <TextView
                            android:id="@+id/hh_textview_q_5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_below="@+id/btn_RedialCall"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_25sdp"
                            android:gravity="right"
                            android:text="ترمیم کرنے کیلئے ممبر منتخب کریں "
                            android:textColor="@color/OrangeRed"
                            android:textSize="@dimen/_20sdp"
                            android:textStyle="bold"


                            />



                        <EditText
                            android:id="@+id/hh_edtfield_q_5"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/hh_textview_q_5"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:background="@drawable/edittextbgwhitecolor"
                            android:digits="98"
                            android:visibility="gone"
                            android:inputType="number"
                            android:maxLength="3"
                            android:paddingLeft="10dp"
                            android:scaleType="fitXY"
                            android:textColorHint="@color/LightGrey"></EditText>



                        <EditText
                            android:id="@+id/hh_edtfield_q_2"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/hh_textview_q_5"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:background="@drawable/edittextbgwhitecolor"
                            android:enabled="false"
                            android:inputType="textPersonName"
                            android:maxLength="500"
                            android:paddingLeft="10dp"
                            android:scaleType="fitXY"
                            android:textColorHint="@color/LightGrey"></EditText>


                        <Spinner

                            android:id="@+id/numbers_sp_q_2"
                            android:layout_width="@dimen/_450sdp"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/hh_edtfield_q_2"
                            android:layout_centerVertical="true"
                            android:layout_marginTop="20dp"
                            android:background="#e6e6e6"
                            android:entries="@array/array_relative_code_sp"
                            android:padding="@dimen/_5sdp"

                            android:textSize="@dimen/_13sdp"></Spinner>


                        <LinearLayout
                            android:id="@+id/phone_number_ll"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/numbers_sp_q_2"
                            android:layout_marginTop="10dp"
                            android:orientation="vertical">

                        </LinearLayout>



                        <TextView
                            android:id="@+id/hh_textview_q_3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_below="@+id/hh_edtfield_q_5"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_25sdp"
                            android:gravity="right"
                            android:text="cehck"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_20sdp"
                            android:visibility="gone"
                            android:textStyle="bold"


                            />


                        <EditText
                            android:id="@+id/sign_up_confirm_email_textfield"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/hh_textview_q_3"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:background="@drawable/edittextbgwhitecolor"
                            android:inputType="text"
                            android:maxLength="150"
                            android:paddingLeft="10dp"
                            android:scaleType="fitXY"
                            android:visibility="gone"
                            android:textColorHint="@color/LightGrey"></EditText>




                        <RadioGroup
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/hh_textview_q_3"
                            android:layout_alignParentRight="true"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:id="@+id/hh_edtfield_q_5_rdg"
                            android:visibility="gone"

                            >

                            <RadioButton
                                android:id="@+id/hh_edtfield_q_5_rdg_op_1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:layout_below="@+id/migration1"
                                android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                                android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                                android:layout_marginTop="@dimen/_15sdp"
                                android:layout_marginBottom="@dimen/_10sdp"
                                android:gravity="right"
                                android:text="cehck"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_14sdp"
                                android:textStyle="bold"
                                android:clickable="true"
                                android:onClick="setMigration1"



                                />

                            <RadioButton
                                android:id="@+id/hh_edtfield_q_5_rdg_op_2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_below="@+id/migration1"
                                android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                                android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                                android:layout_marginTop="@dimen/_15sdp"
                                android:layout_marginBottom="@dimen/_10sdp"
                                android:gravity="right"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_14sdp"
                                android:textStyle="bold"
                                android:clickable="true"
                                android:onClick="setMigration2"

                                />


                            <RadioButton
                                android:id="@+id/hh_edtfield_q_5_rdg_op_3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_below="@+id/migration1"
                                android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                                android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                                android:layout_marginTop="@dimen/_15sdp"
                                android:layout_marginBottom="@dimen/_10sdp"
                                android:gravity="right"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_14sdp"
                                android:textStyle="bold"
                                android:clickable="true"
                                android:onClick="setMigration3"

                                />




                            <RadioButton
                                android:id="@+id/hh_edtfield_q_5_rdg_op_4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_below="@+id/migration1"
                                android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                                android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                                android:layout_marginTop="@dimen/_15sdp"
                                android:layout_marginBottom="@dimen/_10sdp"
                                android:gravity="right"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_14sdp"
                                android:textStyle="bold"
                                android:clickable="true"
                                android:onClick="setMigration4"

                                />


                        </RadioGroup>







                        <TextView
                            android:id="@+id/hh_textview_q_4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_below="@+id/hh_edtfield_q_5_rdg"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_25sdp"
                            android:gravity="right"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_20sdp"
                            android:visibility="gone"
                            android:textStyle="bold"


                            />


                        <EditText
                            android:id="@+id/hh_edtfield_q_4"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/hh_textview_q_4"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:background="@drawable/edittextbgwhitecolor"
                            android:visibility="gone"
                            android:inputType="text"
                            android:maxLength="150"
                            android:paddingLeft="10dp"
                            android:scaleType="fitXY"
                            android:textColorHint="@color/LightGrey"></EditText>








                        <TextView
                            android:id="@+id/head_count"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_below="@+id/sign_up_confirm_email_textfield"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_75sdp"
                            android:gravity="right"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_20sdp"
                            android:textStyle="bold"
                            android:visibility="gone"


                            />

                        <EditText
                            android:id="@+id/sign_up_password_textfield"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/head_count"

                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:background="@drawable/edittextbgwhitecolor"
                            android:visibility="gone"
                            android:inputType="number"
                            android:maxLength="4"
                            android:paddingLeft="10dp"
                            android:scaleType="fitXY"
                            android:textColorHint="@color/LightGrey"></EditText>


                        <TextView
                            android:id="@+id/migration"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_below="@+id/sign_up_password_textfield"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_25sdp"
                            android:gravity="right"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_18sdp"
                            android:textStyle="bold"
                            android:visibility="gone"

                            />

                        <RadioGroup
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/migration"
                            android:layout_alignParentRight="true"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:id="@+id/sign_up_confirm_password_textfield_rb"

                            >

                            <RadioButton
                                android:id="@+id/migration1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:layout_below="@+id/migration1"
                                android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                                android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:gravity="right"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_14sdp"
                                android:textStyle="bold"
                                android:clickable="true"
                                android:onClick="setMigration1"
                                android:visibility="gone"


                                />

                            <RadioButton
                                android:id="@+id/migration2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_below="@+id/migration1"
                                android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                                android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:gravity="right"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_14sdp"
                                android:textStyle="bold"
                                android:clickable="true"
                                android:onClick="setMigration2"
                                android:visibility="gone"

                                />
                        </RadioGroup>

                        <EditText
                            android:id="@+id/sign_up_confirm_password_textfield"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/migration"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:background="@drawable/edittextbgwhitecolor"

                            android:digits="12"
                            android:inputType="number"
                            android:maxLength="1"
                            android:paddingLeft="10dp"
                            android:scaleType="fitXY"
                            android:textColorHint="@color/LightGrey"
                            android:visibility="gone"></EditText>


                        <TextView
                            android:id="@+id/house_address"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_below="@+id/sign_up_confirm_password_textfield_rb"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_20sdp"
                            android:gravity="right"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_18sdp"
                            android:textStyle="bold"
                            android:visibility="gone"


                            />


                        <TextView
                            android:id="@+id/house_no"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_below="@+id/house_address"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:gravity="right"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_20sdp"
                            android:textStyle="bold"
                            android:visibility="gone"


                            />


                        <EditText
                            android:id="@+id/house_no_textfield"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/house_no"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:background="@drawable/edittextbgwhitecolor"
                            android:inputType="text"
                            android:maxLength="500"
                            android:paddingLeft="10dp"
                            android:scaleType="fitXY"
                            android:textColorHint="@color/LightGrey"
                            android:visibility="gone"></EditText>


                        <TextView
                            android:id="@+id/block_no"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_below="@+id/house_no_textfield"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_25sdp"
                            android:gravity="right"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_20sdp"
                            android:textStyle="bold"
                            android:visibility="gone"

                            />


                        <EditText
                            android:id="@+id/block_no_textfield"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/block_no"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:background="@drawable/edittextbgwhitecolor"
                            android:inputType="text"
                            android:maxLength="100"
                            android:paddingLeft="10dp"
                            android:scaleType="fitXY"
                            android:textColorHint="@color/LightGrey"
                            android:visibility="gone"></EditText>


                        <TextView
                            android:id="@+id/area_no"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_below="@+id/block_no_textfield"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_25sdp"
                            android:gravity="right"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_20sdp"
                            android:textStyle="bold"

                            android:visibility="gone" />


                        <EditText
                            android:id="@+id/area_no_textfield"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/area_no"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginRight="@dimen/sign_in_textfield_right_left_margin"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:background="@drawable/edittextbgwhitecolor"
                            android:inputType="text"
                            android:maxLength="200"
                            android:paddingLeft="10dp"
                            android:scaleType="fitXY"
                            android:textColorHint="@color/LightGrey"
                            android:visibility="gone"></EditText>


                        <TextView
                            android:id="@+id/landmark"
                            android:layout_width="wrap_content"
